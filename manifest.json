{
  "manifest_version": 3,
  "author": "kazuya gosho",
  "name": "acro5piano",
  "description": "my chrome extension",
  "version": "0.0.1",
  "homepage_url": "https://github.com/acro5piano/chrome-extension",
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["./sites/github.com/index.js"]
    },
    {
      "matches": ["https://secure.freee.co.jp/*"],
      "js": ["./sites/secure.freee.co.jp/index.js"]
    },
    {
      "matches": ["https://www.notion.so/*"],
      "js": ["./sites/www.notion.so/index.js"]
    },
    {
      "matches": ["https://meet.google.com/*"],
      "js": ["./sites/meet.google.com/index.js"]
    },
    {
      "matches": ["https://analytics.amplitude.com/*"],
      "css": ["./sites/analytics.amplitude.com/index.css"]
    }
  ]
}
