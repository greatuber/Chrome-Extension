{
  "manifest_version": 3,
  "author": "kazuya gosho",
  "name": "acro5piano",
  "description": "my chrome extension",
  "version": "0.0.1",
  "homepage_url": "https://github.com/acro5piano/chrome-extension",
  "content_scripts": [
    {
      "matches": [
        "https://analytics.amplitude.com/*"
      ],
      "css": [
        "./sites/analytics.amplitude.com/index.css"
      ]
    },
    {
      "matches": [
        "https://app.gather.town/*"
      ],
      "js": [
        "./sites/app.gather.town/index.js"
      ]
    },
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "./sites/github.com/index.js"
      ],
      "css": [
        "./sites/github.com/index.css"
      ]
    },
    {
      "matches": [
        "https://majinjima.ma-jide.com/*"
      ],
      "js": [
        "./sites/majinjima.ma-jide.com/index.js"
      ]
    },
    {
      "matches": [
        "https://meet.google.com/*"
      ],
      "js": [
        "./sites/meet.google.com/index.js"
      ]
    },
    {
      "matches": [
        "https://secure.freee.co.jp/*"
      ],
      "js": [
        "./sites/secure.freee.co.jp/index.js"
      ],
      "css": [
        "./sites/secure.freee.co.jp/index.css"
      ]
    },
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "css": [
        "./sites/twitter.com/index.css"
      ]
    },
    {
      "matches": [
        "https://www.notion.so/*"
      ],
      "js": [
        "./sites/www.notion.so/index.js"
      ]
    }
  ]
}